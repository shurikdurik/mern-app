  "license": "ISC",
  "bugs": {
    "url": "https://github.com/shurikdurik/mern-app/issues"
  },
  "homepage": "https://github.com/shurikdurik/mern-app#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "config": "^3.3.9",
    "eslint": "^8.33.0",
    "express": "^4.18.2",
    "express-validator": "^6.14.3",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^6.8.4",
    "shortid": "^2.2.16"
  },
  "devDependencies": {
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
